// Temporary schema for CLI commands (no url in datasource)
// Use this with `--url` to supply the DB connection from the CLI.

generator client {
    provider = "prisma-client-js"
    output   = "/app/generated/prisma-client"
}

datasource db {
    provider = "postgresql"
}

model User {
    id        Int      @id @default(autoincrement())
    createdAt DateTime @default(now())
    email     String   @unique
    name      String?
}
